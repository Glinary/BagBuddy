    <div class="button-container">
        <button class="btn item-gallery" id="item-gallery-btn">
            ITEM GALLERY
        </button>
        <button class="btn edit-profile" id="edit-profile-btn">
            EDIT PROFILE
        </button>
    </div>


    <div class="btn-signout" id="signout-btn">
        sign out
    </div>
</section>

